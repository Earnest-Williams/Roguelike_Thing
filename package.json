{
  "name": "roguelike-thing",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "npm run build && npm run test:run",
    "test-basic": "npm run test",
    "test:run": "node tests/run-basic-tests.js",
    "test:html": "npm run build && npm run test:run | node tests/make-latest.js",
    "test:all": "npm run test:html",
    "build": "tsc -p tsconfig.legacy.json && tsc",
    "typecheck": "tsc -p tsconfig.legacy.json --noEmit && tsc --noEmit",
    "dev": "node dev/dev-server.js"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
